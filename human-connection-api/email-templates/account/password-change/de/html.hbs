{{> header }}
<tr>
	<td class="content-wrap">
		<meta itemprop="name" content="Password change"/>
		<table width="100%" cellpadding="0" cellspacing="0">
		  <tr>
				<td class="aligncenter content-block">
					<h1 class="aligncenter">Passwort geändert</h1>
				</td>
			</tr>
			<tr>
				<td class="aligncenter content-block">
          Hallo {{ name }}, dein passwort wurde eben geändert.
				</td>
			</tr>
			<tr>
				<td class="aligncenter content-block">
					Wenn das nicht du warst <a href='mailto:{{returnEmail}}?subject=I did not change my password&body=Someone unauthorized changed my password. Can you send me a reset link?'>lass es uns wissen!</a>
				</td>
			</tr>
			<tr>
				<td class="aligncenter content-block">
					&mdash; Human Connection
				</td>
			</tr>
		</table>
	</td>
</tr>
{{> footer }}
